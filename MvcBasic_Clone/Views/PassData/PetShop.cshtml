
@{
    ViewBag.Title = "PetShop";
}

<h2>PetShop</h2>

公司名稱：@ViewData["Company"]
<br />
公司地址：@ViewBag.Address
<br />
販賣的寵物有：
<br />
<ul>
    @foreach (var pet in ViewData.Model)
    {
        <li>@pet</li>
    }
</ul>

<h2>訊息摘要：</h2>

<lu>
    <li>使用者：@TempData["User"]</li>
    <li>時間：@TempData["Time"]</li>
    <li>訊息：@TempData["ErrorMessageText"]</li>
</lu>
